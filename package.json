{
  "name": "expensetracker",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "export NODE_ENV=production && node server.js",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "clientinstall": "npm install --prefix client",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "prod": "npm run clientinstall && npm install && npm run build --prefix client && pm2 start server.js",
    "trial":"npm run build --prefix client && pm2 start server.js",
    "stop":"pm2 stop server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.6",
    "pm2": "^4.4.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4"
  }
}
